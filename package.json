{
  "name": "messenger",
  "version": "0.4.0",
  "description": "Messenger for practicum",
  "author": "Kosimov Anvar",
  "license": "ISC",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "resolutions": {
    "trim-newlines": "3.0.1",
    "semver-regex": "3.1.3"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "clean:output": "rimraf dist",
    "build": "npm run clean:output && webpack --mode=production",
    "dev": "webpack serve --mode=development",
    "server": "node server.js",
    "test": "mocha -r ts-node/register -r jsdom-global/register -r ignore-styles src/**/*.spec.ts",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "eslint": "node_modules/.bin/eslint . --ext js,ts",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all eslint:fix stylelint:fix test"
    }
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/jsdom": "16.2.14",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "ajv": "8.11.0",
    "assert": "2.0.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "chai": "4.3.6",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "9.2.0",
    "console-browserify": "1.2.0",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "express": "4.18.1",
    "express-static-gzip": "2.1.5",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "ignore-styles": "5.0.1",
    "image-minimizer-webpack-plugin": "3.2.3",
    "imagemin": "8.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "10.0.1",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "2.6.0",
    "mocha": "10.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sass": "1.51.0",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "stylelint": "14.8.1",
    "stylelint-config-sass-guidelines": "9.0.1",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "util": "0.12.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "nanoid": "3.3.3"
  }
}
